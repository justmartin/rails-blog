<div class="container">

	<div class="row center-align">
		<!-- displays current user account info -->
		<h3>Your Account</h3>
		<h5>First Name</h5>
		<%= current_user.first_name %>
		<br>
		<h5>Last Name</h5>
		<%= current_user.last_name %>
		<br>
		<h5>Email</h5>
		<%= current_user.email %>
		<br>
		<h5>Password</h5>
		<%= current_user.password %>

		<br>
		<br>

		<form action="/edit-account">
	    <button class="btn">Edit Account</button>
		</form>
	</div>

</div>