<div class="container">
	<h1>All Users</h1>

	<!-- displays all users -->
  <% @users.each do |user| %>
	  <div class="row">
			<div class="col s12 m6">
	    	<div class="card">
	        <div class="card-content">
	          <span class="card-title"><%= user.first_name %> <%= user.last_name %></span>
	          <br>
	          <%= link_to "View User", user %>
	        </div>
	      </div>
	    </div>
	  </div>
	<% end %>
	
</div>